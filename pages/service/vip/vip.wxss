/* pages/service/vip/vip.wxss */
page{
  background-color: white;
}
.main{
  display: flex;
  flex-direction: column;
  padding: 8px;
}
.item{
  margin: 8px;
  display: flex;
  flex-direction: column;
  border-radius: 10px;
  box-shadow: 0rpx 10rpx 50rpx 10rpx #5555551A;
}
.item-store{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-left: 15px;
  padding-right: 15px;
  padding-top: 10px;
  padding-bottom: 10px;
  align-items: center;
}
.item-store-row{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.item-store-image{
  width: 25px;
  height: 25px;
}
.item-store-image-right{
  width: 15px;
  height: 15px;
}
.item-store-title{
  font-size: 14px;
  color: black;
  margin-left: 5px;
}
.item-store-content{
  font-size: 14px;
  color: #999999;
}
.item-vip{
  width: 100%;
  height: 180px;
  border-radius: 10px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background-size: 100% 100%;
}
.item-vip-row{
  margin: 20px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.item-vip-column{
  margin: 20px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.item-vip-column-item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.item-vip-name{
  font-size: 24px;
  color: white;
}
.item-vip-discount{
  font-size: 18px;
  color: white;
}
.item-vip-update{
  font-size: 12px;
  color: white;
}
.item-vip-money{
  font-size: 12px;
  color: #FFB11C;
}
.item-vip-progress{
  margin-top: 10px;
  margin-bottom: 10px;
}
.item-vip-tips{
  font-size: 10px;
  color: #DDDDDD;
}